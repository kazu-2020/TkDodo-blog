    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "typescript": "^3.9.7",
    "ts-jest": "^26.1.3",
    "vue-jest": "^3.0.6"
  },
  "pre-commit": [
    "precommit-msg",
    "lintfix"
  ]
}
