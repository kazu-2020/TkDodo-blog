{
  "name": "editorial-hands",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "tsc --noEmit && vite build --mode dev",
    "build:stg": "tsc --noEmit && vite build --mode stg",
    "build:prd": "tsc --noEmit && vite build --mode prd",
    "preview": "vite preview",
    "lint": "eslint --ext .tsx,.ts src/",
    "format": "prettier --write \"**/*.+(js|json|yml|ts|tsx)\"",
    "test": "vitest",
    "snapshot": "vitest run -u",
    "coverage": "vitest run --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.2",
    "@chakra-ui/react": "^2.2.1",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@storybook/react": "^6.5.9",
    "@vitejs/plugin-react": "^1.3.2",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "framer-motion": "^6.3.11",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.33.1",
    "react-icons": "^4.4.0",
    "react-query": "^3.39.1",
    "react-router-dom": "6",
    "type-fest": "^2.14.0",
    "vite": "^2.9.12"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@faker-js/faker": "^7.3.0",
    "@mswjs/data": "^0.10.1",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-vite": "^0.1.38",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-loader": "^8.2.5",
    "c8": "^7.11.3",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-storybook": "^0.5.13",
    "eslint-plugin-testing-library": "^5.5.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest-extended": "^2.0.0",
    "jsdom": "^20.0.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3",
    "vitest": "^0.15.2"
  },
  "main": "src/main.tsx"
}
