{
  "name": "editorial-hands",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "tsc --noEmit && vite build --mode dev",
    "build:stg": "tsc --noEmit && vite build --mode stg",
    "build:prd": "tsc --noEmit && vite build --mode prd",
    "preview": "vite preview",
    "lint": "eslint --ext .tsx,.ts src/",
    "format": "prettier --write \"**/*.+(js|json|yml|ts|tsx)\"",
    "test": "vitest",
    "snapshot": "vitest run -u",
    "coverage": "vitest run --coverage",
    "storybook": "start-storybook -p 6006",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "npx concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"yarn start-storybook -p 6006 --ci\" \"npx wait-on http://localhost:6006 && yarn test-storybook\"",
    "test-storybook:cix": "npx concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpm build-storybook --quiet && npx http-server storybook-static --port 6006\" \"npx wait-on http://127.0.0.1:6006 && pnpm test-storybook --maxWorkers=2\"",
    "build-storybook": "build-storybook",
    "webstorm-integration": "vitest --watch --reporter=dot --reporter=json --outputFile=.vitest-result.json"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.2",
    "@chakra-ui/react": "^2.2.1",
    "@dnd-kit/core": "^6.0.5",
    "@dnd-kit/sortable": "^7.0.1",
    "@dnd-kit/utilities": "^3.2.0",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@storybook/react": "^6.5.10",
    "@vitejs/plugin-react": "^2.0.0",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "framer-motion": "^6.3.11",
    "immer": "^9.0.15",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.33.1",
    "react-icons": "^4.4.0",
    "react-query": "^3.39.1",
    "react-router-dom": "6",
    "snakecase-keys": "^5.4.2",
    "type-fest": "^2.14.0",
    "use-react-router-breadcrumbs": "^3.2.1",
    "vite": "^3.0.4",
    "zustand": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@faker-js/faker": "^7.3.0",
    "@mswjs/data": "^0.10.1",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/jest": "^0.0.10",
    "@storybook/test-runner": "^0.5.0",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitest/ui": "^0.20.3",
    "babel-loader": "^8.2.5",
    "c8": "^7.11.3",
    "eslint": "^8.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.3",
    "eslint-plugin-testing-library": "^5.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "27",
    "jest-extended": "^2.0.0",
    "jsdom": "^20.0.0",
    "msw-storybook-addon": "^1.6.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vitest": "^0.20.3"
  },
  "main": "src/main.tsx",
  "msw": {
    "workerDirectory": "public"
  }
}
