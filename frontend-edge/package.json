{
  "name": "editorial-hands",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "tsc --noEmit && vite build --mode dev",
    "build:stg": "tsc --noEmit && vite build --mode stg",
    "build:prd": "tsc --noEmit && vite build --mode prd",
    "preview": "vite preview",
    "lint": "eslint --ext .tsx,.ts src/",
    "lint:tsc": "tsc --noEmit",
    "format": "prettier --write \"**/*.+(js|json|yml|ts|tsx)\"",
    "test": "vitest",
    "snapshot": "vitest run -u",
    "coverage": "vitest run --coverage",
    "storybook": "start-storybook -p 6006",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "npx concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpm start-storybook -p 6006 --ci\" \"npx wait-on http://localhost:6006 && pnpm test-storybook\"",
    "build-storybook": "build-storybook",
    "webstorm-integration": "vitest --watch --reporter=dot --reporter=json --outputFile=.vitest-result.json"
  },
  "dependencies": {
